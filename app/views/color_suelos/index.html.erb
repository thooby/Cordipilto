<h1>Listing color_suelos</h1>

<table>
  <tr>
    <th>Descripcion</th>
  </tr>

<% @color_suelos.each do |color_suelo| %>
  <tr>
    <td><%=h color_suelo.descripcion %></td>
    <td><%= link_to 'Show', color_suelo %></td>
    <td><%= link_to 'Edit', edit_color_suelo_path(color_suelo) %></td>
    <td><%= link_to 'Destroy', color_suelo, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New color_suelo', new_color_suelo_path %>