<h1>Listing invier_accesos</h1>

<table>
  <tr>
    <th>Descripcion</th>
  </tr>

<% @invier_accesos.each do |invier_acceso| %>
  <tr>
    <td><%=h invier_acceso.descripcion %></td>
    <td><%= link_to 'Show', invier_acceso %></td>
