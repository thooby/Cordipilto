<h1>Listing alfabes</h1>

<table>
  <tr>
    <th>Descripcion</th>
  </tr>

<% @alfabes.each do |alfabe| %>
  <tr>
    <td><%=h alfabe.descripcion %></td>
    <td><%= link_to 'Show', alfabe %></td>
    <td><%= link_to 'Edit', edit_alfabe_path(alfabe) %></td>
    <td><%= link_to 'Destroy', alfabe, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New alfabe', new_alfabe_path %>